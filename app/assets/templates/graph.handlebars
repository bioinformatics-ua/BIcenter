<style>
    .nav-tabs > li .close {
        margin: -2px 0 0 10px;
        font-size: 18px;
    }
</style>
<div class="box box-default">
    <div class="box-body">

        {{> toolbar}}

        <!-- Transformation Pipeline -->
        <div view-element="main_page">
            <div id="selectActions" valign="top" style="display:none;">
                <!-- Select Actions Here -->
            </div>
            <div id="mainActions" valign="top" style="display:none;">
                <!-- Main Actions Here -->
            </div>
            <table border="0" style="height:100%;width:100%;">
                <tr>
                    <td id="toolbar" style="display:none;" valign="top">
                        <!-- Toolbar Here -->
                    </td>
                    <td id="board" view-element="board" valign="top"
                        style="height:60vh;width:100%;border-width:1px;border-style:solid;border-color:black;">
                        <div id="graph" view-element="graph" tabindex="-1"
                             style="background-image:url('data:image/svg+xml;base64,');border-color: rgb(255, 255, 255); border-style: solid; background-color: rgb(255, 255, 255);height:100%;width:100%;position:relative;overflow:hidden;cursor:default;">
                            <!-- Graph Here -->
                        </div>
                        <textarea id="xml" view-element="xml"
                                  style="height:100%;width:100%;display:none;border-style:none;"></textarea>
                    </td>
                </tr>
            </table>
            <input id="source" view-element="source" type="checkbox" style="display:none;"/>
            <button id="xmlBtn" view-element="xmlBtn" type="button" class="btn bg-wet-asphalt margin" view-click="loadXml();"
                    style="display:none;float:right">Import
            </button>
            <div id="zoomActions" style="display:none;">
            </div>
        </div>
    </div>
</div>