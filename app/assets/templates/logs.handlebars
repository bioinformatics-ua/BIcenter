<div class="box box-default">
    <div class="box-header with-border">
        <h3 class="box-title">Execution Logs</h3>
    </div>

    <div class="box-body">
        <form class="form-horizontal">
            <textarea view-element="preview_log" class="form-control" rows="20" placeholder="Enter ...">{{#task.execution}}{{#ifCond logText '!=' null}}{{logText}}{{/ifCond}}{{/task.execution}}</textarea>
        </form>
    </div>
    <!-- /.box-body -->
    <div class="box-footer">
        <button view-click="controller.cancelClick()" type="submit" class="btn bg-wet-asphalt margin pull-right">Ok</button>
    </div>
    <!-- /.box-footer -->
</div>