<div class="box box-default">
    <div class="box-header with-border">
        <h3 class="box-title">Preview Data</h3>
    </div>

    <div class="box-body">
        <table view-element="preview_table" border="0" style="height:100%;width:100%;">
            <tr>
                <td id="toolbar" style="display:none;" valign="top">
                    <!-- Toolbar Here -->
                </td>
                <td id="board" view-element="preview_board" valign="top" style="height:58vh;width:100%;border-width:1px;border-style:solid;border-color:black;">
                    <div view-element="preview_graph" style="background-image:url('data:image/svg+xml;base64,');border-color: rgb(255, 255, 255); border-style: solid; background-color: rgb(255, 255, 255);height:100%;width:100%;position:relative;overflow:hidden;cursor:default;">
                        <!-- Graph here -->
                    </div>
                    <textarea id="xml" view-element="xml" style="height:100%;width:100%;display:none;border-style:none;"></textarea>
                </td>
            </tr>
        </table>
    </div>

    <!-- /.box-body -->
    <div class="box-footer">
        <button view-click="controller.cancelClick()" type="submit" class="btn bg-wet-asphalt margin pull-right">Ok</button>
    </div>
    <!-- /.box-footer -->
</div>